import java.util.*;
public class changeCase {    
    public static void main(String[] args) {    
            
        Scanner scn = new Scanner(System.in);
        char c = scn.next().charAt(0);
        
        if(c>=97 && c<=122){
            char UC=(char)(c-'a'+'A');
            System.out.println(UC);
        }
        else{
            char LC=(char)(c-'A'+'a');
            System.out.println(LC);
        }
    }    
}     
